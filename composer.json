{
  "name": "santosdave/sabre-wrapper",
  "description": "Laravel wrapper for Sabre APIs",
  "type": "library",
  "keywords": [
    "santosdave",
    "sabre-wrapper",
    "sabre",
    "ndc",
    "api",
    "travel"
  ],
  "require": {
    "php": "^8.1",
    "ext-soap": "*",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "monolog/monolog": "^3.8",
    "laravel/framework": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Santosdave\\SabreWrapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Santosdave\\SabreWrapper\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "authors": [
    {
      "name": "santosdave",
      "email": "51959957+santosdave@users.noreply.github.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Santosdave\\SabreWrapper\\SabreServiceProvider"
      ],
      "aliases": {
        "Sabre": "Santosdave\\SabreWrapper\\Facades\\Sabre"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
